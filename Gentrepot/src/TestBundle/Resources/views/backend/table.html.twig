{% extends 'backend/index.html.twig' %}
{% block title %} table {% endblock %}


{% block content %}

<section class="section">
    <div class="container-fluid">

        <div class="row">
            <div class="col-md-8 col-md-offset-2">

                <p></p>

                <div class="panel">
                    <div class="panel-heading">
                        <div class="panel-title">
                            <h5>Basic Example</h5>
                        </div>
                    </div>
                    <div class="panel-body p-20">

                        <table class="table">
                            <caption>Optional table caption.</caption>
                            <thead>
                            <tr>
                                <th>#</th>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Username</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <th scope="row">1</th>
                                <td>Mark</td>
                                <td>Otto</td>
                                <td>@mdo</td>
                            </tr>
                            <tr>
                                <th scope="row">2</th>
                                <td>Jacob</td>
                                <td>Thornton</td>
                                <td>@fat</td>
                            </tr>
                            <tr>
                                <th scope="row">3</th>
                                <td>Larry</td>
                                <td>the Bird</td>
                                <td>@twitter</td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
{% endblock %}